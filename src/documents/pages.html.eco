---
layout: 'content'
title: 'Pages'
---

<div class="ui row">
	<h1 class="header"><%- @document.title or @document.name %></h1>
</div>

<div class="ui row">
	<ul class="ui list">
		<% for item in @getCollection('pages').sortArray(date: -1): %>
		<a class="item" href="<%= item.url %>">
			<%- item.title or item.name %></a>
		<%- item.description %>
		<% end %>
	</ul>
</div>

<%- @partial('contribute') %>
