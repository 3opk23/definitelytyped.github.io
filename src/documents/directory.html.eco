---
layout: 'content'
title: 'TypeScript Directory'
---

<div class="ui row">
	<h1 class="header"><%- @document.title or @document.name %></h1>
</div>

<div class="ui row">
	<div class="ui list">
		<% for item in @getCollection('directory').sortArray(date: -1): %>
		<a class="item" href="<%= item.url %>">
			<%- item.title or item.name %></a>
		<%- item.description %>
		<% end %>
	</div>
</div>

<%- @partial('contribute') %>
